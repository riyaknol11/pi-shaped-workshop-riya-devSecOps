{
  "name": "devsecops-demo",
  "version": "1.0.0",
  "description": "DevSecOps Demo - Secure version with environment variables",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "security-audit": "npm audit",
    "security-fix": "npm audit fix",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:build": "docker build -t devsecops-demo .",
    "docker:run": "docker run -p 3000:3000 --env-file .env devsecops-demo",
    "docker:compose": "docker-compose up --build",
    "prestart": "node -e \"require('dotenv').config(); const missing = ['API_KEY', 'JWT_SECRET', 'DB_PASSWORD'].filter(v => !process.env[v]); if (missing.length) { console.error('Missing env vars:', missing.join(', ')); process.exit(1); }\""
  },
  "keywords": [
    "devsecops",
    "security",
    "nodejs",
    "environment-variables",
    "secure-coding"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.0"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/devsecops-demo.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/devsecops-demo/issues"
  },
  "homepage": "https://github.com/your-username/devsecops-demo#readme"
}
